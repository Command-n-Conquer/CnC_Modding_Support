	<!-- Unit Definitions. A set of ObjectFilters to describe each type of unit -->
	<FormationAssistantUnitDefinition id="Inf">
		<IncludeObjectFilter
			Rule="NONE"
			Include="INFANTRY"
			Exclude="ENGINEER SIEGE_WEAPON">
			<ExcludeThing>GDIInfantryU</ExcludeThing>
		</IncludeObjectFilter>
	</FormationAssistantUnitDefinition>

	<FormationAssistantUnitDefinition id="Cap">
		<IncludeObjectFilter
			Rule="NONE"
			Include="ENGINEER">
			<IncludeThing>GDIInfantryU</IncludeThing>
			<IncludeThing>NODVehicleAE</IncludeThing>
		</IncludeObjectFilter>
	</FormationAssistantUnitDefinition>

	<FormationAssistantUnitDefinition id="Cav">
		<IncludeObjectFilter
			Rule="NONE"
			Include="VEHICLE"
			Exclude="AIRCRAFT SIEGE_WEAPON HARVESTER">
			<ExcludeThing>NODVehicleAE</ExcludeThing>
		</IncludeObjectFilter>
	</FormationAssistantUnitDefinition>

	<FormationAssistantUnitDefinition id="Artillery">
		<IncludeObjectFilter
			Rule="NONE"
			Include="SIEGE_WEAPON">
		</IncludeObjectFilter>
	</FormationAssistantUnitDefinition>

	<FormationAssistantUnitDefinition id="Aircraft">
		<IncludeObjectFilter
			Rule="NONE"
			Include="AIRCRAFT"
		/>
	</FormationAssistantUnitDefinition>

