	<!-- Base micromanager. Inherit from this, do not use directly -->
	<AIMicroManagerData
		id="BaseMicroManager"
		UpdateTime="0.5s"
		MaxTeamIdleTime="15s"
		MaxTeamIdleDistanceChange="50.0" 
		MaxUnitIdleTime="10s"
		MaxUnitIdleDistanceChange="30.0"
		EnemySearchDistanceVisionMultiplier="1.5"
		MinTimeToKeepTempVisionRangeBonus="10s"
		EnemyHealerValueFudgeFactor="20.0"
		SiegeWeaponBuffer="1.5"
		DodgeThreatsReactionTimeMin="0.5s"
		DodgeThreatsReactionTimeMax="0.5s"
		GatherTempVisionRangeBonus="1.5"
		RetreatTempVisionRangeBonus="1.5"
		MaxSecondaryDamageFraction="1.3"
		OverrideSpecialPowersAllowed="CRAWLER_UNPACK CRAWLER_PACK"
		TacticControlledSpecialPowersAllowed="MOVEMENT SPEED_BOOST">
		
		<!-- Prefer to retreat to goal nodes first. Then to either captured nodes or our spawn zone. -->
		<RetreatBaseCriteria
			PreferredBaseTypes="GAMEPLAY_STRUCTURE_BASE INCLUDES_GOAL_STRUCTURE" 
			PreferredNotBaseTypes="CAPTURED_BASE"
			SortOrder="PREFER_CLOSEST" />

		<!-- Remove this once we get a real fix for the AI attacking goals inappropriately -->
		<IgnoreTargets
			Rule="ANY"
			Include="GOAL_STRUCTURE" />

		<CrateCollectionInfo
			UnitScoreStartingValue="6.0"
			DistanceScoreFactor="300.0"
			DistanceScoreDivisor="50.0"
			ShouldAvoidSwitchingPenalty="1.0"
			UpgradeCrateRequiredClass="HEAVY"
			RejectUnitForUpgradeCratePickupByStatus="HAS_CRATE_UPGRADE AI_SKIP_UPGRADE_CRATE_SEARCH"
			CrateAvoidanceDistance="100.0"
			CratePickupDelay="3s"/>

		<HealInfo
			ScoreDistanceMinRange="100.0"
			ScoreDistanceMaxRange="300.0"
			ScoreDistanceMinRangeIdle="150.0"
			ScoreDistanceMaxRangeIdle="350.0"
			ScoreSkirmishAIValueMax="500"
			HealRetargetDelay="8s"
			HealStartMinimumHealthPriorityThreshold="50%"
			Options="NO_STACKING_HEALS"
			StatusToRejectUnitForHeal="BURROWING DISABLED_FOR_RECYCLE" />

		<InitialJoinInfo
			DistanceThreshold="=$INITIAL_JOIN_DISTANCE"
			TeamCenterDistanceDiffThreshold="=$INITIAL_JOIN_TEAMCENTER_CHANGE_THRESHOLD" />

	</AIMicroManagerData>

