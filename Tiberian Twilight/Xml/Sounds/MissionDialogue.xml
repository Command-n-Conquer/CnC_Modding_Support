            Volume="110" 
            MinVolume="100"
            Limit="1"
            Type="WORLD EVERYONE"
            Priority="CRITICAL" 
            SubmixSlider="VOICE"
			Buss="DMission"
            Control="FADE_ON_KILL" >
            <VolumeSliderMultiplier Slider="VOICE" Multiplier="0.4" /> 
            <VolumeSliderMultiplier Slider="SOUNDFX" Multiplier="0.7" /> 
            <VolumeSliderMultiplier Slider="MUSIC" Multiplier="0.7" /> 
            <VolumeSliderMultiplier Slider="AMBIENT" Multiplier="0.95" /> 
        </AudioEvent>

    <!-- Defaults for Mission Color Dialogue. Derive Mission Color Dialogue from this one.  These are the settings for "colorful commentary" mission dialog, a little bit quieter and won't duck any other sounds -->
