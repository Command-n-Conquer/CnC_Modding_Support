	<!-- Defaults for Mission LOST Dialogue. More ducking than the normal Mission Dialogue. -->
        <AudioEvent id="BaseMissionLostDialogue" inheritFrom="AudioEvent:BaseSoundEffect"
            Volume="110" 
            MinVolume="100"
            Limit="1"
            Type="WORLD EVERYONE"
            Priority="CRITICAL" 
            SubmixSlider="VOICE"
			Buss="DMission"
            Control="FADE_ON_KILL" >
            <VolumeSliderMultiplier Slider="VOICE" Multiplier="0.3" /> 
            <VolumeSliderMultiplier Slider="SOUNDFX" Multiplier="0.6" /> 
            <VolumeSliderMultiplier Slider="MUSIC" Multiplier="0.6" /> 
            <VolumeSliderMultiplier Slider="AMBIENT" Multiplier="0.95" /> 
        </AudioEvent>

    <!-- Defaults for Mission Dialogue. Derive Mission Dialogue from this one -->
