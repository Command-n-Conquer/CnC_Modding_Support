            Volume="100" 
            MinVolume="90"
            Limit="1"
            Type="WORLD EVERYONE"
            Priority="CRITICAL" 
			Buss="DMission"
            SubmixSlider="VOICE"
            Control="FADE_ON_KILL" >
        </AudioEvent>

