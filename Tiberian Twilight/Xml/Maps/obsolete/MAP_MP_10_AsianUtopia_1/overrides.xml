<?xml version="1.0" encoding="UTF-8"?>
<AssetDeclaration
	xmlns="uri:ea.com:eala:asset"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xai="uri:ea.com:eala:asset:instance">
	
	<Tags/>
	<Includes>
		<Include type="instance" source="DATA:GlobalData/BaseGameplayMode.xml" />
	</Includes>

	<GameplayMode
		xai:joinAction="Append"
		id="MAP_MP_10_AsianUtopia_1_DOM"
		Mode="DOMINATION"
		AllowRespawn="true"
		inheritFrom="Domination_Base">
	</GameplayMode>
	
	<GameplayMode
		xai:joinAction="Append"
		id="MAP_MP_10_AsianUtopia_1_MON"
		Mode="MONUMENT"
		AllowRespawn="true"
		inheritFrom="Monument_Base">

		<PointInfoDestroy
			Target="Monument"
			Shield="MonumentShieldGenerator"
			TargetHealth="150000"
			ShieldAmount="10000"
			ShieldRechargePerSecond="500"
			ShieldRechargeDelay="0s"/>
	</GameplayMode>
	
	<GameplayMode
		xai:joinAction="Append"
		id="MAP_MP_10_AsianUtopia_1_CAH"
		Mode="CAPTURE_AND_HOLD"
		AllowRespawn="true"
		inheritFrom="CaptureAndHold_Base">
	</GameplayMode>

</AssetDeclaration>