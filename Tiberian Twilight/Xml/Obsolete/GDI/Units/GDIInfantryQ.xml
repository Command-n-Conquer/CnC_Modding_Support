		<!-- Base Object -->
		<Include
			type="instance"
			source="DATA:BaseObjects/BaseInfantry.xml" />
	</Includes>
	<GameObject
		id="GDIInfantryQ"
		inheritFrom="BaseInfantry"
		SelectPortrait="Portrait_GDIEngineer"
		ButtonImage="Portrait_GDIEngineer"
		Side="GDI"
		EditorSorting="UNIT"
		TransportSlotCount="1"		
		BuildTime="5"
		CommandSet="EmptyCommandSet"
		CommandPoints="0"
		IsTrainable="true"
		KindOf="+CAN_CAPTURE +SKIRMISH_AI_DONT_GARRISON +ENGINEER +HOTKEY_CYCLE_UNIT +CAN_BE_FAVORITE_UNIT -CAN_ATTACK -SELECTABLE"
		RadarPriority="UNIT"
		ThreatLevel="10"
		UnitCategory="INFANTRY"
		VoicePriority="68"
		EditorName="GDIInfantryQ"
		Description="Desc:GDIInfantryQ"
		TypeDescription="Type:GDIInfantryQ"
		UnitTypeIcon="EngineerIcon">
		<ObjectResourceInfo>
			<BuildCost Account="=$ACCOUNT_CREDIT" Amount="500"/>
		</ObjectResourceInfo>
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">Name:GDIInfantryQ</DisplayName>
		<!--<Prerequisites
			Object="GDIBarracks" />-->
		<ArmorSet
			Armor="LightArmor"
			DamageFX="InfantryDamageFX" />
		<LocomotorSet
			Locomotor="HumanLocomotor"
			Condition="NORMAL"
			Speed="50" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="GUEngineer_SKN" />
				</ModelConditionState>
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="INSIDE_GARRISON">
					<Model
						Name="" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FORMATION_PREVIEW">
					<!-- no anim -->
				</AnimationState>
				
				
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					AllowRepeatInRandomPick="true"
					StateName="STATE_Bored">
					<Animation
						AnimationName="GUEngineer_BIDA"
						AnimationMode="ONCE"
						AnimationPriority="10" />
					<Animation
						AnimationName="GUEngineer_BIFA"
						AnimationMode="ONCE"
						AnimationPriority="1" />
					<Animation
						AnimationName="GUEngineer_BIFB"
						AnimationMode="ONCE"
						AnimationPriority="5" />
					<Animation
						AnimationName="GUEngineer_BIFC"
						AnimationMode="ONCE"
						AnimationPriority="5" />
					<Animation
						AnimationName="GUEngineer_BIFD"
						AnimationMode="ONCE"
						AnimationPriority="1" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_Bored" or Prev == "TRANS_Deselected" then
						--Do Nothing and Re-Pick Animation 
						elseif Prev == "STATE_stunned_flailing" then
						CurDrawableSetTransitionAnimState("STATE_stunned_landing")
						else
						CurDrawableSetTransitionAnimState("TRANS_Deselected")
						end 
						
					
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_FLAILING"
					StateName="STATE_stunned_flailing">
					<Animation
						AnimationName="GUEngineer_FLYA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					StateName="STATE_stunned_landing">
					<Animation
						AnimationName="GUEngineer_LNDA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FRONTCRUSHED">
					<Animation
						AnimationName="GUEngineer_CDTA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="BACKCRUSHED">
					<Animation
						AnimationName="GUEngineer_CDTB"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Animation
						AnimNickName="DTA"
						AnimationName="GUEngineer_DIEA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="GUEngineer_RUNA"
						AnimationMode="LOOP"
						AnimationSpeedFactorMin="1.0"
						AnimationSpeedFactorMax="1.0" />
				</AnimationState>
				<!-- EMOTIONS -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="EMOTION_CELEBRATING"
					Flags="RESTART_ANIM_WHEN_COMPLETE">
					<Animation
						AnimationName="GUEngineer_CHRA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="GUEngineer_CHRB"
						AnimationMode="ONCE" />
				</AnimationState>
				<!--Selected-->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SELECTED"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_Selected">
					<Animation
						AnimationName="GUEngineer_AIDA"
						AnimationMode="ONCE"
						AnimationPriority="15"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_AIFA"
						AnimationMode="ONCE"
						AnimationPriority="5"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_AIFB"
						AnimationMode="ONCE"
						AnimationPriority="5"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Animation
						AnimationName="GUEngineer_AIFC"
						AnimationMode="ONCE"
						AnimationPriority="2"
						AnimationBlendTime="5"
						AnimationMustCompleteBlend="true" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_Bored" then
						CurDrawableSetTransitionAnimState("TRANS_boredToAttention")
						end
					</Script>
				</AnimationState>
				<!-- TRANSITIONS -->
				
				<AnimationState
         ParseCondStateType="PARSE_TRANSITION"
         StateName="TRANS_Deselected">
          <Animation
						AnimationName="GUEngineer_DSEL"
						AnimationMode="ONCE"
						AnimationBlendTime="5" />
        </AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_boredToAttention">
					<Animation
						AnimationName="GUEngineer_BATA"
						AnimationMode="ONCE" />
				</AnimationState>
				<!--
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_attentionToBored">
					<Animation
						AnimationName="GUEngineer_ABTA"
						AnimationMode="ONCE" />
				</AnimationState>
				-->
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<Physics
				id="ModuleTag_07" />
			<SlowDeath
				id="ModuleTag_Death"
				SinkDelay="6s"
				SinkRate="2.4"
				DestructionDelay="7.5s">
				<DieMuxData
					DeathTypes="ALL" />
				<Sound
					Type="INITIAL"
					List="GDI_Engineer_VoiceDie" />
			</SlowDeath>
			<LifetimeUpdate
				id="ModuleTag_LifetimeUpdate"
				MinLifetime="30s"
				MaxLifetime="30s"
				DeathType="FADED" />
		</Behaviors>
		<AI>
			<AIUpdate
				id="ModuleTag_06"
				AutoAcquireEnemiesWhenIdle="NO"
				StateMachine="UnitAIStateMachine"/>
		</AI>
		<Body>
			<ActiveBody
				id="ModuleTag_03"
				MaxHealth="150.0" />
		</Body>
		<Geometry
			IsSmall="true">
			<Shape
				Type="CYLINDER"
				MajorRadius="7.0"
				MinorRadius="7.0"
				Height="13.0">
				<Offset
					x="0"
					y="0"
					z="0" />
			</Shape>
		</Geometry>
		<AudioArrayVoice>
			<AudioEntry
				Sound="GDI_Engineer_VoiceCreate"
				AudioType="voiceCreated" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceGarrison"
				AudioType="voiceGarrison" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceMove"
				AudioType="voiceMove" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceRepair"
				AudioType="voiceRepair" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceRetreat"
				AudioType="voiceRetreatToCastle" />
			<AudioEntry
				Sound="GDI_Engineer_VoiceSelectMS"
				AudioType="voiceSelect" />
			<!-- Voice for being ordered to enter and repair or capture various things. The names are defined in the 
			     various EngineerContain modules on the target objects -->
			<NamedEntry
				Sound="GDI_Engineer_VoiceRepair"
				Name="VoiceEngineerRepair" />
			<NamedEntry
				Sound="GDI_Engineer_VoiceCaptureBuilding"
				Name="VoiceCaptureBuilding" />
			<NamedEntry
				Sound="GDI_Engineer_VoiceCaptureHusk"
				Name="VoiceCaptureHusk" />
		</AudioArrayVoice>
		<!--
		<AudioArraySound>
			<AudioEntry Sound="AudioEvent:TEMP_PredatorMoveStart" AudioType="soundMoveStart" />
			<AudioEntry Sound="AudioEvent:PredatorAmbientLoop" AudioType="soundAmbient" />
		</AudioArraySound>
	-->
		<ShadowInfo
			Type="DECAL"
			SizeX="14"
			SizeY="14"
			Texture="ShadowI" />
		<VisionInfo
			VisionRange="50"
			ShroudClearingRange="100" />
	</GameObject>
</AssetDeclaration>
