	<GameObject
		id="NODInfantryQ"
		inheritFrom="BaseInfantry"
		SelectPortrait="Portrait_NOD_Infantry_Q"
		ButtonImage="Btn_NOD_Infantry_Q"
		Side="NOD"
		EditorSorting="UNIT"
		TransportSlotCount="1"
		BuildTime="0.1"
		CommandSet="EmptyCommandSet"
		KindOf="+SIEGE_WEAPON +CAN_SHOOT_OVER_WALLS"
		RadarPriority="UNIT"
		ThreatLevel="10"
		UnitCategory="INFANTRY"
		CombatChainWeaponCategory="FIELD_ARTILLERY"
		WeaponCategory="BLAST"
		VoicePriority="88"
		EditorName="NODInfantryQ"
		Description="Desc:NODInfantryQ"
		TypeDescription="Type:NODInfantryQ">
		<ObjectResourceInfo>
			<BuildCost Account="=$ACCOUNT_CREDIT" Amount="150"/>
		</ObjectResourceInfo>
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">NAME:NODInfantryQ</DisplayName>
		<ArmorSet
			Armor="GDIInfantryEArmor"
			DamageFX="InfantryDamageFX" />
		<LocomotorSet
			Locomotor="HumanLocomotor"
			Condition="NORMAL"
			Speed="54" />
		<SkirmishAIInformation
			UnitBuilderStandardCombatUnit="true" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="GUGrendr_SKN" />
					<WeaponLaunchBone
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="Props0" />
				</ModelConditionState>
				<xi:include
					href="DATA:Includes/GDIUnitFormationPreviewStates.xml"
					xpointer="xmlns(n=uri:ea.com:eala:asset) xpointer(/n:FormationPreviewStates/child::*)" />
				<!-- invisible when garrisoned -->
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="INSIDE_GARRISON">
					<Model
						Name="" />
					<WeaponLaunchBone
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UPGRADED_ARMOR">
					<Model
						Name="GUGrendr_SKN" />
					<Texture
						Original="GuGrenadier3"
						New="GuGrenadier3U" />
				</ModelConditionState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="INSIDE_GARRISON">
					<!-- no anim -->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					AllowRepeatInRandomPick="true"
					StateName="STATE_bored">
					<Animation
						AnimationName="GUGrendr_BIDA"
						AnimationMode="ONCE"
						AnimationPriority="50" />
					<Animation
						AnimationName="GUGrendr_BIDB"
						AnimationMode="ONCE"
						AnimationPriority="20" />
					<Animation
						AnimationName="GUGrendr_BIDC"
						AnimationMode="ONCE"
						AnimationPriority="20" />
					<Animation
						AnimationName="GUGrendr_BIFA"
						AnimationMode="ONCE"
						AnimationPriority="25" />
					<Animation
						AnimationName="GUGrendr_BIFB"
						AnimationMode="ONCE"
						AnimationPriority="20" />
					<Animation
						AnimationName="GUGrendr_BIFC"
						AnimationMode="ONCE"
						AnimationPriority="20" />
					<Animation
						AnimationName="GUGrendr_BIFD"
						AnimationMode="ONCE"
						AnimationPriority="20" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_SelectedPose" then
						CurDrawableSetTransitionAnimState("TRANS_SelectedToIdle")
						end
						if Prev == "STATE_stunned_flailing" then
						CurDrawableSetTransitionAnimState("STATE_stunned_landing")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_FLAILING"
					StateName="STATE_stunned_flailing">
					<Animation
						AnimationName="GUGrendr_FLYA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					StateName="STATE_stunned_landing">
					<Animation
						AnimationName="GUGrendr_LNDA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FRONTCRUSHED">
					<Animation
						AnimationName="GUGrendr_CDTA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="BACKCRUSHED">
					<Animation
						AnimationName="GUGrendr_CDTB"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Animation
						AnimationName="GUGrendr_DIEA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="GUGrendr_DIEB"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING FIRING_OR_PREATTACK_A"
					FrameForPristineBonePositions="31">
					<Animation
						AnimationName="GUGrendr_RUNA"
						AnimationMode="LOOP" />
					<Script>
						CurDrawableSetTransitionAnimState("TRANS_AttackingRunning")
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FIRING_OR_PREATTACK_A"
					FrameForPristineBonePositions="31">
					<Animation
						AnimationName="GUGrendr_ATKA"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />
					<!--<Animation
						AnimationName="GUGrendr_ATKB"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />
					<Animation
						AnimationName="GUGrendr_ATKC"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />
					<Animation
						AnimationName="GUGrendr_ATKD"
						AnimationMode="ONCE"
						UseWeaponTiming="true" />-->
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="GUGrendr_RUNA"
						AnimationMode="LOOP"
						Distance="75" />
					<Animation
						AnimationName="GUGrendr_RUNB"
						AnimationMode="LOOP"
						Distance="75" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ATTACKING">
					<Animation
						AnimationName="GUGrendr_AIDA"
						AnimationMode="LOOP" />
				</AnimationState>
				<!-- CELEBRATING -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="EMOTION_CELEBRATING">
					<Animation
						AnimationName="GUGrendr_CHRA"
						AnimationMode="LOOP"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="GUGrendr_CHRB"
						AnimationMode="LOOP"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="GUGrendr_CHRC"
						AnimationMode="LOOP"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
				</AnimationState>
				<!-- SELECTED -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SELECTED"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_SelectedPose">
					<Animation
						AnimationName="GUGrendr_AIDA"
						AnimationMode="ONCE"
						AnimationBlendTime="10" />
					<Animation
						AnimationName="GUGrendr_AIDB"
						AnimationMode="ONCE"
						AnimationBlendTime="5" />
					<Animation
						AnimationName="GUGrendr_AIFA"
						AnimationMode="ONCE"
						AnimationBlendTime="5" />
					<Animation
						AnimationName="GUGrendr_AIFB"
						AnimationMode="ONCE"
						AnimationBlendTime="5" />
					<Animation
						AnimationName="GUGrendr_AIFC"
						AnimationMode="ONCE"
						AnimationBlendTime="5" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_bored" then
							CurDrawableSetTransitionAnimState("TRANS_IdleToSelected")
						end
					</Script>
				</AnimationState>
				<!-- Duplicate AnimationState -->
				<!-- <AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ATTACKING">
					<Animation
						AnimationName="GUGrendr_AIFA"
						AnimationMode="LOOP" />
				</AnimationState> -->
				<!-- TRANSITIONS -->
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_AttackingRunning">
					<Animation
						AnimationName="GUGrendr_ATRA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_IdleToSelected">
					<Animation
						AnimationName="GUGrendr_BATA"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="0.9"
						AnimationSpeedFactorMax="1.1" />
					<Animation
						AnimationName="GUGrendr_BATB"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="0.9"
						AnimationSpeedFactorMax="1.1" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_SelectedToIdle">
					<Animation
						AnimationName="GUGrendr_ABTA"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="0.9"
						AnimationSpeedFactorMax="1.1" />
					<Animation
						AnimationName="GUGrendr_ABTA2"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="0.9"
						AnimationSpeedFactorMax="1.1" />
				</AnimationState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<WeaponSetUpdate
				id="ModuleTag_WeaponSetUpdate">
				<WeaponSlotHardpoint
					ID="1"
					AllowInterleavedFiring="false"
					InterleavedStyle="INTERLEAVE_FIRST_AVAILABLE"
					WeaponChoiceCriteria="PREFER_MOST_DAMAGE">
					<Weapon
						Ordering="PRIMARY_WEAPON"
						Template="GDIInfantryEGrenade" />
				</WeaponSlotHardpoint>
			</WeaponSetUpdate>
			<Physics
				id="ModuleTag_Physics" />
			<SlowDeath
				id="ModuleTag_Death"
				SinkDelay="3s"
				SinkRate="2.4"
				DestructionDelay="4.5s">
				<DieMuxData
					DeathTypes="ALL" />
				<Sound
					Type="INITIAL"
					List="GDI_Generic_VoiceDieMS" />
			</SlowDeath>

		</Behaviors>
		<AI>
			<AIUpdate
				id="ModuleTag_AI"
				AutoAcquireEnemiesWhenIdle="YES"
				StateMachine="UnitAIStateMachine">
				<UnitAITargetChooserData
					SympathyRange="100.0" />
			</AIUpdate>
		</AI>
		<Body>
			<ActiveBody
				id="ModuleTag_Body"
				MaxHealth="380" />
		</Body>
    <ClientUpdates>
      <LargeGroupAudioClientUpdate
        id="LGAU">
        <Key>GDI_GrenadierSoldier</Key>
      </LargeGroupAudioClientUpdate>
    </ClientUpdates>   
