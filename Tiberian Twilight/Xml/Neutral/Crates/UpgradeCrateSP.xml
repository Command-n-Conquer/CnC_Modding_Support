<?xml version="1.0" encoding="us-ascii"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Tags></Tags>
	<!-- 
	Only used for SP, should have an unlimited life timer with the purpose of designers pre-placing them in their missions 
	-->
	<Includes>
		<Include
			type="instance"
			source="DATA:Neutral/Crates/UpgradeCrate.xml" />
	</Includes>
	<GameObject
		id="UpgradeCrateSP"
		inheritFrom="UpgradeCrate"
		EditorName="UpgradeCrateSP">
		<DisplayName
			xai:joinAction="Replace" >NAME:UpgradeCrateSP</DisplayName>
		<Behaviors>
			<LifetimeUpdate xmlns="uri:ea.com:eala:asset"
			id="ModuleTag_LifetimeUpdate"
			xai:joinAction="Remove"/>
		</Behaviors>
	</GameObject>
</AssetDeclaration>