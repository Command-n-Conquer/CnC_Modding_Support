<?xml version="1.0" encoding="utf-8"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xai="uri:ea.com:eala:asset:instance">
	<!-- NOD Offense Crawler, Structure Mode -->
	<Tags></Tags>
	<Includes>
		<Include type="all" source="ART:NSX_CrawlerB_SKN.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_UT1.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_UT2.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_UT3.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_UT1_IDLA.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_UT2_IDLA.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_UT2_IDLB.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_UT3_IDLB.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_T1_BLDA.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_T2_BLDA.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_T3_BLDA.w3x" />
		<Include type="all"	source="ART:NSX_CrawlerB_UT1-UT2.w3x" />
		<Include type="all" source="ART:NSX_CrawlerB_UT2-UT3.w3x" />
		<Include type="all" source="ART:NUX_CrawlerB_SKN.w3x" />
		<Include type="all"	source="ART:NUX_CrawlerB_DPLY.w3x" />
		
		<Include type="all" source="ART:FXObeliskLaser.xml" />
		<Include type="all" source="ART:FXLaserOverlay.xml" />
		<Include type="all" source="ART:Decal_Rig_CO.xml" />

		<!-- Laser Draw Includes -->
		<Include type="instance" source="ART:FXNodLaser.xml" />
		<Include type="instance" source="ART:FXNodLaser_Veteran.xml" />
		<Include type="instance" source="ART:FXEnergyBeam.xml" />
		<Include type="instance" source="ART:FXNodLaser_Secondary.xml" />
		<Include type="instance" source="ART:FXNodLaser_Veteran_Secondary.xml" />
		<Include type="instance" source="ART:FXRailDistort02.xml" />
		
		<Include type="instance" source="DATA:BaseObjects/BaseCrawlerStructure.xml" />
		<Include type="instance" source="DATA:UI/HealthBarTemplates.xml" />
	</Includes>
	<GameObject
		id="NODCrawlerBStructure"
		inheritFrom="BaseCrawlerStructure"
		CommandSet="NODCrawlerStructureCommandSet"
		HealthBoxHeightOffset="35"
		HealthBar="Large"
		SubGroupPriority="37"
		EditorName="NOD_O1_Struct_Crawler(Offense Crawler)"
		EditorSorting="STRUCTURE"
		Description="Desc:NODOffenseCrawler"
		TypeDescription="Type:NODOffenseCrawler"
		SelectPortrait="Portrait_NOD_Crawler_B_str_tech1"
		ButtonImage="Btn_NOD_Crawler_B_str_tech1"
		Side="NOD"
		WeaponCategory="LASER"
		KindOf="+MULTIPLE_WEAPON_CATEGORIES" 
		ParticipationValue="=$NODCRAWLERB_PARTICIPATION_VALUE">
		<DisplayName>Name:NODOffenseCrawler</DisplayName>
		<!-- RetreatSpeedBonus should match speed of vehicle form -->
		<SkirmishAIInformation
				RetreatSpeedBonus="50.0" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_MainDrawModule"
				OkToChangeModelColor="true"
				ExtraPublicBone="=$NODCRAWLERB_DAMAGE_BONE_NAMES + ' FX_Weapon_A01 FX_Weapon_B01 FX_Weapon_B02 Tower01 Tower02 Crane03'">

				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="NSX_CrawlerB_SKN" />

					<WeaponFireFXBone
						WeaponSlotID="1"	
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="FX_Weapon_B" />
					<WeaponLaunchBone
						WeaponSlotID="1"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="FX_Weapon_B" />

					<WeaponFireFXBone
						WeaponSlotID="2"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="FX_Weapon_A01" />
					<WeaponRecoilBone
						WeaponSlotID="2"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="FX_Weapon_A01" />
					<WeaponLaunchBone
						WeaponSlotID="2"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="FX_Weapon_A01" />

					<Turret
						TurretNameKey="Turret01"
						TurretPitch="Pitch01"
						TurretID="1" />
					<Turret
						TurretNameKey="Turret02"
						TurretPitch="Pitch02"
						TurretID="2" />

				</ModelConditionState>

				<!-- Vehicle transition, phase 1 -->
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="CRAWLER_TRANSITION_1">
					<Model
						Name="NUX_CrawlerB_SKN" />
				</ModelConditionState>
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="BUILD_PLACEMENT_CURSOR">
					<Animation
						AnimationName="NSX_CrawlerB_UT1_IDLA"
						AnimationMode="LOOP" />
					<Script>
						CurDrawableHideSubObjectPermanently( "TECH3" )
						CurDrawableHideSubObjectPermanently( "TECH2" )
					</Script>
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					StateName="STATE_IDLE">
					<Animation
						AnimationName="NSX_CrawlerB_UT1_IDLA"
						AnimationBlendTime="0"
						AnimationMode="LOOP" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev == "STATE_VEHICLE_TRANSITION" then
							CurDrawableSetTransitionAnimState("TRANS_VehicleTech1ToDoorClosed")
						end
					</Script>
				</AnimationState>

				<!-- Vehicle Unpacking, phase 1 -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="CRAWLER_TRANSITION_1">
					<Animation
						AnimationName="NUX_CrawlerB_DPLY"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="1.0"
						AnimationSpeedFactorMax="1.0" />
					<!-- BURROWING -->
					<FXEvent
						Frame="10"
						FireWhenSkipped="true"
						Effect="FX_NOD_Burrow_Large"
						Bone="" />
				</AnimationState>

				<!-- Vehicle Unpacking, phase 2 -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="CRAWLER_TRANSITION_2 USER_2"
					StateName="STATE_VEHICLE_TRANSITION">
					<Animation
						AnimationName="NSX_CrawlerB_UT3"
						AnimationBlendTime="12"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="1.0"
						AnimationSpeedFactorMax="1.0" />
					<!-- BURROWING -->
					<FXEvent
						Frame="25"
						FireWhenSkipped="true"
						Effect="FX_NOD_BurrowB_Large"
						Bone="Crawler" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="CRAWLER_TRANSITION_2 USER_1"
					StateName="STATE_VEHICLE_TRANSITION">
					<Animation
						AnimationName="NSX_CrawlerB_UT2"
						AnimationBlendTime="12"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="1.0"
						AnimationSpeedFactorMax="1.0" />
					<!-- BURROWING -->
					<FXEvent
						Frame="25"
						FireWhenSkipped="true"
						Effect="FX_NOD_BurrowB_Large"
						Bone="Crawler" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="CRAWLER_TRANSITION_2"
					StateName="STATE_VEHICLE_TRANSITION">
					<Animation
						AnimationName="NSX_CrawlerB_UT1"
						AnimationBlendTime="5"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="1.0"
						AnimationSpeedFactorMax="1.0" />
					<!-- BURROWING -->
					<FXEvent
						Frame="25"
						FireWhenSkipped="true"
						Effect="FX_NOD_BurrowB_Large"
						Bone="Crawler" />
				</AnimationState>

				<!-- units exiting -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_OPENING USER_2">
					<Animation
						AnimationName="NSX_CrawlerB_T3_BLDA"
						AnimationBlendTime="3"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="2"
						AnimationSpeedFactorMax="2" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev == "STATE_VEHICLE_TRANSITION" then
							CurDrawableSetTransitionAnimState("TRANS_VehicleTech3ToDoorOpened")
						end
					</Script>
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_WAITING_OPEN USER_2"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T3_BLDA"
						AnimationMode="MANUAL" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_CLOSING USER_2"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T3_BLDA"
						AnimationMode="ONCE_BACKWARDS" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_OPENING USER_1"
					StateName="DOOR_OPENING_UT2">
					<Animation
						AnimationName="NSX_CrawlerB_T2_BLDA"
						AnimationBlendTime="3"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="2"
						AnimationSpeedFactorMax="2" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev == "STATE_VEHICLE_TRANSITION" then
							CurDrawableSetTransitionAnimState("TRANS_VehicleTech2ToDoorOpened")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_WAITING_OPEN USER_1"
					StateName="DOOR_OPENING_UT2"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T2_BLDA"
						AnimationMode="MANUAL" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_CLOSING USER_1"
					StateName="DOOR_OPENING_UT2"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T2_BLDA"
						AnimationMode="ONCE_BACKWARDS" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_OPENING">
					<Animation
						AnimationName="NSX_CrawlerB_T1_BLDA"
						AnimationBlendTime="3"
						AnimationMode="ONCE"
						AnimationSpeedFactorMin="2"
						AnimationSpeedFactorMax="2" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev == "STATE_VEHICLE_TRANSITION" then
							CurDrawableSetTransitionAnimState("TRANS_VehicleTech1ToDoorOpened")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_WAITING_OPEN"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T1_BLDA"
						AnimationMode="MANUAL" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DOOR_1_CLOSING"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T1_BLDA"
						AnimationMode="ONCE_BACKWARDS" />
				</AnimationState>


				<!-- Tech 2 Idle -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="USER_1"
					StateName="STATE_IDLE_2"
					Flags="RESTART_ANIM_WHEN_COMPLETE">
					<Animation
						AnimationName="NSX_CrawlerB_UT2_IDLA"
						AnimationBlendTime="0"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="NSX_CrawlerB_UT2_IDLB"
						AnimationBlendTime="0"
						AnimationMode="ONCE" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev ~= null and Prev ~= "STATE_IDLE_2" and Prev == "STATE_IDLE" then
						CurDrawableSetTransitionAnimState("TRANS_Tech1ToTech2")
						elseif Prev == "STATE_VEHICLE_TRANSITION" then
						CurDrawableSetTransitionAnimState("TRANS_VehicleTech2ToDoorClosed")
						end
					</Script>
				</AnimationState>

				<!-- Tech 3 Idle -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="USER_2"
					StateName="STATE_IDLE_3">
					<Animation
						AnimationName="NSX_CrawlerB_UT3_IDLB"
						AnimationBlendTime="0"
						AnimationMode="LOOP" />
					<FXEvent
						Frame="98"
						FireWhenSkipped="true"
						Effect="FX_NODConstruction_Sparks"
						Bone="Crane03" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev ~= null and Prev ~= "STATE_IDLE_3" and Prev == "STATE_IDLE_2" then
						CurDrawableSetTransitionAnimState("TRANS_Tech2ToTech3")
						elseif Prev == "STATE_VEHICLE_TRANSITION" then
						CurDrawableSetTransitionAnimState("TRANS_VehicleTech3ToDoorClosed")
						end
					</Script>
				</AnimationState>

				<!-- tech transitions -->
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_Tech1ToTech2">
					<Animation
						AnimationName="NSX_CrawlerB_UT1-UT2"
						AnimationBlendTime="12"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_Tech2ToTech3">
					<Animation
						AnimationName="NSX_CrawlerB_UT2-UT3"
						AnimationBlendTime="12"
						AnimationMode="ONCE" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
				</AnimationState>

				<!-- from-vehicle closedoor transitions -->
				<!-- These transitions are played when converting from a vehicle and no production units have been queued up -->
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_VehicleTech1ToDoorClosed"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T1_BLDA"
						AnimationBlendTime="0"
						AnimationMode="ONCE_BACKWARDS" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<Script>
						CurDrawablePlaySound("NOD_CrawlerBOffense_DeployIrisForBackwardDeployAnimationMS")
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_VehicleTech2ToDoorClosed"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T2_BLDA"
						AnimationBlendTime="0"
						AnimationMode="ONCE_BACKWARDS" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<Script>
						CurDrawablePlaySound("NOD_CrawlerBOffense_DeployIrisForBackwardDeployAnimationMS")
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_VehicleTech3ToDoorClosed"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T3_BLDA"
						AnimationBlendTime="0"
						AnimationMode="ONCE_BACKWARDS" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<Script>
						CurDrawablePlaySound("NOD_CrawlerBOffense_DeployIrisForBackwardDeployAnimationMS")
					</Script>
				</AnimationState>

				<!-- from-vehicle opendoor transitions -->
				<!-- These transitions are played when converting from a vehicle and production units have been queued up -->
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_VehicleTech1ToDoorOpened"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T1_BLDA"
						AnimationBlendTime="0"
						AnimationMode="MANUAL" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_VehicleTech2ToDoorOpened"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T2_BLDA"
						AnimationBlendTime="0"
						AnimationMode="MANUAL" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_VehicleTech3ToDoorOpened"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="NSX_CrawlerB_T3_BLDA"
						AnimationBlendTime="0"
						AnimationMode="MANUAL" />
					<ParticleSysBone
						BoneName="Tower01"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="Tower02"
						FXParticleSystemTemplate="Smokestack_NOD_Crawler"
						FollowBone="true" />
				</AnimationState>
				
			
			</ScriptedModelDraw>
			<xi:include
				href="DATA:NOD/Crawlers/NODCrawlerBCommonModulesDraw.xml"
				xpointer="xmlns(n=uri:ea.com:eala:asset) xpointer(/n:CrawlerDrawModules/child::*)" />
		</Draws>
		<Behaviors>
			<ReplaceSelfSpecialAbility
				id="SpecialPower_CrawlerPackUpdate"
				NewObjectUnpackTime="=$NOD_OFFENSE_CRAWLER_DEPLOY_DISABLE_TIME"
				ReplaceOptions="+COPY_TURRET_INFO">
				<ReplacementTemplate>NODCrawlerBVehicle</ReplacementTemplate>
			</ReplaceSelfSpecialAbility>
			<SlowDeath
				id="ModuleTag_StructureDeath"
				SinkDelay="0.0s"
				SinkRate="0.0"
				DestructionDelay="0.0s">
				<OCL
					Type="FINAL">
					<OCL>NODCrawlerBStructure_Die_OCL</OCL>
				</OCL>
				<DieMuxData
					DeathTypes="ALL" />
				<!-- <Sound
					Type="INITIAL"
					List="NOD_CrawlerBOffense_VoiceDie" /> -->
			</SlowDeath>
			<FXListBehavior
				id="ModuleTag_StructureFXList">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onTransitionToDamaged"
					FX="FX_BuildDamaged_Large" />
				<Event
					Index="onTransitionToReallyDamaged"
					FX="FX_BuildDamaged_Large" />
				<Event
					Index="onTransitionToRubble"
					FX="FX_BuildRubble_Large" />
				<Event
					Index="onDeath"
					FX="FX_NODCrawler_Death" />
				<Event Index="onDeath" FX="FX_Crawler_DieExplosion_AudioOnly" />
			</FXListBehavior>
			<QueueProductionExitUpdate
				id="ModuleTag_QueueProduction">
				<UnitCreatePoint
					x="44.0"
					y="0.0"
					z="0.0" />
			</QueueProductionExitUpdate>
			
			<!-- stealth (and associated Dodge) on Crawler Tech2 -->
			<InvisibilityUpgrade
				id="ModuleTag_Invisibility"
				InvisibilityTemplate="NODCrawlerInvisibilityTemplate">
				<TriggeredBy>Upgrade_Nod_Offense_CrawlerTech2</TriggeredBy>
			</InvisibilityUpgrade>
			<AttributeModifierUpgrade
				id="ModuleTag_StealthDodgeUpgrade"
				AttributeModifier="Modifier_StealthDodge">
				<TriggeredBy>Upgrade_Nod_Offense_CrawlerTech2</TriggeredBy>
			</AttributeModifierUpgrade>
			
			<!-- structure unpack lasts whole deploy+unpack duration -->
			<StructureUnpackUpdate
				id="ModuleTag_StructureUnpackUpdate"
				UnpackTime="=$NOD_OFFENSE_CRAWLER_STRUCTURE_UNPACK_TIME" />


			<!-- make sure this comes before the BuildingDestructionBehavior -->
			<SpecialModelConditionReplaceOfotherBehavior
				id="ModuleTag_SpecialModelConditions">
				<ModelCondition ModelCondition="CRAWLER_TRANSITION_1" Duration="1s"/>
				<ModelCondition ModelCondition="CRAWLER_TRANSITION_2" Duration="2s"/>
			</SpecialModelConditionReplaceOfotherBehavior>

			<!-- make sure this comes after the SpecialModelConditionReplaceOfotherBehavior -->
