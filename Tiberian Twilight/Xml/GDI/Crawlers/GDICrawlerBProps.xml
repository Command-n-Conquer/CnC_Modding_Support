<?xml version="1.0" encoding="us-ascii"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Tags></Tags>
	<Includes>
		<Include type="all" source="ART:GDIRocketMedium.w3x" />
		<Include type="all" source="ART:GDIRocketMedium_Veteran.w3x" />
		<Include type="instance" source="DATA:BaseObjects/BaseProjectile.xml" />
	</Includes>
	<GameObject
		id="GDICrawlerBRocketWeaponProjectile"
		inheritFrom="BaseTargetableProjectile" 
		Side="GDI"
		EditorName="GDICrawlerBRocketWeaponProjectile">
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="GDIRocketMedium" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="GDIMissileMedium_Trail"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="GDIMissileMedium_TrailThin"
						FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<FXListBehavior
				id="ModuleTag_FXLists">
				<DieMuxData
					DeathTypes="ALL" />
				<Event Index="onDeath" FX="FX_GDI_CrawlerBOffense_RocketImpactExplosion_AudioOnly" /> <!-- audio only, please add visuals to another Event, thanks -->
			</FXListBehavior>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				FirstHeightMin="-2"
				FirstHeightMax="-2"
				SecondHeightMin="20"
				SecondHeightMax="20"
				FirstPercentIndent="33"
				SecondPercentIndent="66"
				GroundHitFX="FX_GDIMediumMissleMiss"
				SidewaysDrift="50"
				FlightPathAdjustDistPerSecond="100.0"
				Flags="DETONATE_CALLS_KILL ORIENT_TO_FLIGHT_PATH" >
			</BezierProjectile>
		</Behaviors>
		<Geometry
			IsSmall="true">
			<Shape
				Type="SPHERE"
				MajorRadius="0.8"></Shape>
		</Geometry>
	</GameObject>

	<GameObject
		inheritFrom="GDICrawlerBRocketWeaponProjectile"
		id="GDICrawlerBRocketWeaponProjectile_Veteran"
		EditorName="GDICrawlerBRocketWeaponProjectile_Veteran">
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw"
				xai:joinAction="Replace">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="NODRocketMedium_Veteran" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="NODRocketMedium_Trail"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="NODRocketMedium_TrailThin_Vet"
						FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				GroundHitFX="FX_GDIMediumMissleMiss_Veteran" />
		</Behaviors>
	</GameObject>
</AssetDeclaration>